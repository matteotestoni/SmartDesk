  <SqlScriptCreate>CREATE VIEW dbo.Immobili_Vw
AS
SELECT        dbo.Immobili.*, dbo.ImmobiliCategoria.ImmobiliCategoria_Descrizione, dbo.ImmobiliTipologia.ImmobiliTipologia_Descrizione, dbo.ImmobiliCondizione.ImmobiliCondizione_Descrizione
FROM            dbo.Immobili LEFT OUTER JOIN
                         dbo.ImmobiliCondizione ON dbo.Immobili.ImmobiliCondizione_Ky = dbo.ImmobiliCondizione.ImmobiliCondizione_Ky LEFT OUTER JOIN
                         dbo.ImmobiliTipologia ON dbo.Immobili.ImmobiliTipologia_Ky = dbo.ImmobiliTipologia.ImmobiliTipologia_Ky LEFT OUTER JOIN
                         dbo.ImmobiliCategoria ON dbo.Immobili.ImmobiliCategoria_Ky = dbo.ImmobiliCategoria.ImmobiliCategoria_Ky
</SqlScriptCreate>
