<%@ Page codepage="65001" Language="C#" AutoEventWireup="true" CodeFile="/admin/view.aspx.cs" Inherits="_Default" Debug="true"%><!DOCTYPE html><html class="no-js" lang="it"><head>	<title><%=strH1%></title>	<!--#include file="/admin/inc-head.aspx"--></head><body><!--#include file=/admin/inc-mainbar.aspx --> <div data-sticky-container>  <div class="content-header" data-sticky="content-header" data-margin-top="0" data-margin-bottom="1" style="width:100%">    <div class="grid-x grid-padding-x align-middle">        <div class="large-5 medium-5 small-12 cell align-middle">          	<h1><i class="fa-duotone fa-inbox fa-lg fa-fw"></i><%=strH1%></h1>            <!--#include file=/admin/view-inc-grids.aspx -->         </div>        <div class="large-7 medium-7 small-12 cell float-right align-middle">        		<div class="button-group small hide-for-print align-right">        			<a class="button clear" data-toggle="filtri"><i class="fa-duotone fa-magnifying-glass fa-lg fa-fw"></i>Cerca</a>        			<a href="/admin/goto-view.aspx?CoreEntities_Ky=258" class="tiny button clear"><i class="fa-duotone fa-calendar-day fa-lg fa-fw"></i>Prenotazioni</a>        			<a href="/admin/form.aspx?CoreModules_Ky=29&CoreEntities_Ky=109&CoreGrids_Ky=284&CoreForms_Ky=1216&custom=0&azione=new&sorgente=elenco-veicoli" class="tiny button"><i class="fa-duotone fa-square-plus fa-lg fa-fw"></i>Nuovo</a>        		</div>	      </div>    </div>  </div></div><div class="grid-x grid-padding-x">  <div class="large-12 medium-12 small-12 cell">    <!--#include file=/admin/app/amministrazione/where/where-spese.inc -->     <div class="divgrid">    <div class="grid-x grid-padding-x">      <div class="large-9 medium-9 small-12 cell">			    <asp:Label ID="PaginaSotto" runat="server" class="paginazione align-middle"></asp:Label>      </div>      <div class="large-3 medium-3 small-12 cell">        <form name="azionidigruppo-form" id="azionidigruppo-form" class="form-azione" method="post" action="#">        <div class="input-group">          <select class="input-group-field" name="azionidigruppo" id="azionidigruppo">            <option value="">Azioni di gruppo</option>            <option value="delete" data-action="/admin/crud/elimina.aspx">Elimina</option>          </select>          <div class="input-group-button">            <input type="hidden" name="grid" value="elencoautosostitutive">            <input type="hidden" id="sorgente" name="sorgente" value="view-1">            <input type="hidden" id="azione" name="azione" value="">            <input type="hidden" name="ajax" value="">            <input type="hidden" id="deletemultiplo" name="deletemultiplo" value="deletemultiplo">            <input type="hidden" id="azionidigruppo-ids" name="azionidigruppo-ids" value="">            <input type="submit" id="doaction" class="button secondary" value="Applica">          </div>        </div>        </form>      </div>    </div>        <table id="elencoautosostitutive" class="grid hover scroll" border="0" width="100%">    	<thead>	      <tr>	        <th width="10"><input type="checkbox" id="selectall" name="selectall" /></th>	        <th width="50">Codice</th>        	<th width="30" class="text-center">Foto</th>        	<th width="110" class="text-left">Vettura</th>        	<th width="80" class="text-center">Targa</th>        	<th width="110" class="text-left">Riferimento</th>        	<th width="110" class="text-left">Situazione</th>        	<th width="110" class="text-left">Carburante</th>        	<th width="110" class="text-left">Cambio</th>        	<th width="110" class="text-left">Normativa</th>        	<th width="110" class="text-left">Carrozzeria</th>	        <th width="12" class="nowrap" data-orderable="false"></th>	      </tr>    	</thead>    	<tbody>		    <%        	System.Data.DataTable dtVeicoliPrenotazioni;                  for (int i = 0; i < dtGridData.Rows.Count; i++){ %>		        <tr class="riga<%=i%2%>">		          <td><input type="checkbox" class="checkrow" id="record<%=dtGridData.Rows[i]["Veicoli_Ky"].ToString()%>" data-value="<%=dtGridData.Rows[i]["Veicoli_Ky"].ToString()%>" /></td>		          <td><a href="/admin/form.aspx?CoreModules_Ky=29&CoreEntities_Ky=109&CoreGrids_Ky=284&CoreForms_Ky=1216&custom=0&azione=edit&sorgente=elenco-autosostitutive&Veicoli_Ky=<%=dtGridData.Rows[i]["Veicoli_Ky"].ToString()%>"><%=dtGridData.Rows[i]["Veicoli_Ky"].ToString()%></a></td>		          <td><img src="<%=dtGridData.Rows[i]["Veicoli_Foto1"].ToString()%>" height="30" style="max-width:100px;height:30px;max-height:30px"></td>		          <td class="text-left"><%=dtGridData.Rows[i]["VeicoliMarca_Titolo"].ToString()%> <%=dtGridData.Rows[i]["VeicoliModello_Titolo"].ToString()%></td>		          <td class="text-center"><%=dtGridData.Rows[i]["Veicoli_Targa"].ToString()%></td>		          <td class="text-left"><%=dtGridData.Rows[i]["Veicoli_Riferimento"].ToString()%></td>		          <td class="text-left">                <%                  strFROMNet = "VeicoliPrenotazioni_Vw";                  strORDERNet = "VeicoliPrenotazioni_Ky";                  strWHERENet = "Veicoli_Ky=" + dtGridData.Rows[i]["Veicoli_Ky"].ToString() + " AND VeicoliPrenotazioni_DataFine Is Null";                  dtVeicoliPrenotazioni = Smartdesk.Sql.getTablePage(strFROMNet, null, "VeicoliPrenotazioni_Ky", strWHERENet, strORDERNet, 1,1,Smartdesk.Config.Sql.ConnectionReadOnly, out this.intNumRecords);                              if (dtVeicoliPrenotazioni.Rows.Count<1){                    Response.Write("<span class=\"label success\">Libera</span>");                  }else{                    Response.Write("<span class=\"label warning\">Occupata fino al " + dtVeicoliPrenotazioni.Rows[0]["VeicoliPrenotazioni_DataRientroPrevista"].ToString() + " </span>");                  }                %>              </td>		          <td class="text-left"><%=dtGridData.Rows[i]["VeicoliCarburante_Descrizione"].ToString()%></td>		          <td class="text-left"><%=dtGridData.Rows[i]["VeicoliCambio_Descrizione"].ToString()%></td>		          <td class="text-left"><%=dtGridData.Rows[i]["VeicoliNormativeEuro_Descrizione"].ToString()%></td>		          <td class="text-left"><%=dtGridData.Rows[i]["VeicoliCarrozzeria_Descrizione"].ToString()%></td>                            		          <td><a href="/admin/crud/elimina.aspx?azione=delete&ajax=&CoreGrids_Ky=1&sorgente=view-284&Veicoli_Ky=<%=dtGridData.Rows[i]["Veicoli_Ky"].ToString()%>" title="elimina" class="delete"><i class="fa-duotone fa-trash-can fa-fw"></i></a></td>		        </tr>		    <% } %>    	</tbody>    </table>    </div>  </div> </div><!--#include file=/admin/inc-footer.aspx --></body></html>